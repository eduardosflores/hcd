{% extends 'base.html.twig' %}

{% block title %}
    Asingar número a Decreto
{% endblock title %}
{% block body %}
    <h1>Asingar número a  Decreto</h1>
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Decreto</h3>
        </div>
        <div class="box-body">

            {{ form_start(edit_form) }}
            {{ form_row(edit_form.numero) }}
            <input class="btn btn-primary" type="submit" value="Guardar"/>
            <div class="hidden">
                {{ form_end(edit_form) }}
            </div>
        </div>
    </div>

    <a class="btn btn-default" href="{{ path('decreto_index') }}">Volver</a>



{% endblock %}
