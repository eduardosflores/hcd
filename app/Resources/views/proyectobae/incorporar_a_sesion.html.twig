{% extends 'base.html.twig' %}

{% block title %}
    Incorporar a Sesión
{% endblock title %}
{% block body %}
    <h1>Incorporar a Sesión</h1>
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Expediente {{ expediente }}</h3>
        </div>
        <div class="box-body">

            <h3>Sesiones</h3>
            {% for bae in expedienteEnSesiones %}
                - {{ bae.boletinAsuntoEntrado.sesion }} <a class="btn btn-default" href="{{ path('proyectobae_edit' ,{'id': bae.id}) }}"><i class="fa fa-edit"></i></a><br>
            {% endfor %}
            <br>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <input class="btn btn-primary" type="submit" value="Guardar"/>
            {{ form_end(form) }}
        </div>
    </div>

    <a class="btn btn-default" href="{{ path('proyectobae_index') }}">Volver</a>

{% endblock %}
